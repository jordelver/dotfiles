  excludesfile = "~/.gitignore"
  editor = mate -w
[alias]
  st = status
  co = checkout
  ci = commit
  br = branch -a
  a = add
  staged = diff --staged
  l = log --oneline --max-count=10
  last = log -1 HEAD
  d = diff
  dt = difftool
